@{
    
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    
    <table cellpadding="5" width="100%">
        
            @foreach(var i in Model)
            {
                <tr>
                    <td>
                        @i.Id
                    </td>
                    <td>
                        @i.DealerName
                    </td>
                    <td>
                        @i.DealerAddress
                    </td>
                    <td>
                        @i.DealerAccountId
                    </td>
                    <td>
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                                <a class="nav-link text-dark" style="border:solid" asp-area="" asp-controller="Home" asp-action="Edit" asp-route-id="@i.Id">Edit</a>
                        </li>
                    </ul>
                    </td>
                </tr>
            }
    </table>
</div>
