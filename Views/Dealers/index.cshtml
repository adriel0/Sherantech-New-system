<table cellpadding="5" width="100%">

    @foreach (var i in Model)
    {
        <tr>
            <td>
                @i.Id
            </td>
            <td>
                @i.DealerName
            </td>
            <td>
                @i.DealerAddress
            </td>
            <td>
                @i.DealerAccountId
            </td>
            <td>
                <ul class="navbar-nav flex-grow-1">
                    <li class="nav-item">
